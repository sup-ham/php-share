{
    "name": "supham/php-share",
    "keywords": ["composer", "shared library", "symlink"],
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Supham\\Phpshare\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "class": "Supham\\Phpshare\\ComposerPlugin"
    }
}
